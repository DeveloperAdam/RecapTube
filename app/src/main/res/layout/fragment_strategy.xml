<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="androidlab.com.recaptube.Fragments.MainTabs.StrategyFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="7dp">

        <Button
            android:id="@+id/brainstormed"
            android:layout_width="315dp"
            android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="brainstormed"/>

        <Button
            android:id="@+id/developed"
            android:layout_width="315dp"
            android:layout_height="wrap_content"
            android:text="developed"
            android:textAllCaps="false"
            android:layout_toRightOf="@+id/brainstormed"/>

        <Button
            android:id="@+id/explored"
            android:layout_width="315dp"
            android:layout_height="wrap_content"
            android:text="explored"
            android:textAllCaps="false"
            android:layout_toRightOf="@+id/developed"/>

        <Button
            android:id="@+id/identified"
            android:layout_width="315dp"
            android:layout_height="wrap_content"
            android:text="identified"
            android:textAllCaps="false"
            android:layout_below="@+id/brainstormed"
            android:layout_marginTop="10dp"/>

        <Button
            android:id="@+id/outlined"
            android:layout_width="315dp"
            android:layout_height="wrap_content"
            android:text="outlined"
            android:layout_below="@+id/developed"
            android:layout_toRightOf="@+id/identified"
            android:textAllCaps="false"
            android:layout_marginTop="10dp"/>

        <Button
            android:id="@+id/reviewed"
            android:layout_width="315dp"
            android:layout_height="wrap_content"
            android:text="reviewed"
            android:layout_below="@+id/explored"
            android:layout_toRightOf="@+id/outlined"
            android:textAllCaps="false"
            android:layout_marginTop="10dp"/>



        <Button
            android:id="@+id/straitegized"
            android:layout_width="315dp"
            android:layout_height="wrap_content"
            android:text="strategized"
            android:textAllCaps="false"
            android:layout_below="@+id/identified"
            android:layout_marginTop="10dp"/>



        <Button
            android:id="@+id/suggested"
            android:layout_width="315dp"
            android:layout_height="wrap_content"
            android:text="suggested"
            android:textAllCaps="false"
            android:layout_below="@+id/outlined"
            android:layout_toRightOf="@+id/straitegized"
            android:layout_marginTop="10dp"/>

    </RelativeLayout>

</LinearLayout>
